<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="imaging_sequence" model="ir.sequence">
            <field name="name">Imaging Request</field>
            <field name="code">imaging_seq</field>
            <field name="prefix">IMG</field>
            <field name="padding">3</field>
        </record>

        <record id="inpatient_registration_code_sequence" model="ir.sequence">
            <field name="name">Patients</field>
            <field name="code">hospital.inpatient.registration</field>
            <field name="prefix">INPAC</field>
            <field name="padding">3</field>
        </record>

        <record id="hospital_appointment_code_sequence" model="ir.sequence">
            <field name="name">Appointment</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">APT</field>
            <field name="padding">0</field>
        </record>

        <record id="hospital_health_service_code_sequence" model="ir.sequence">
            <field name="name">Service</field>
            <field name="code">hospital.health_service</field>
            <field name="prefix">SERV%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="patient_registration_sequence" model="ir.sequence">
            <field name="name">Patient Seq</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">PAC</field>
            <field name="padding">2</field>
        </record>

        <record id="prescription_order_sequence" model="ir.sequence">
            <field name="name">Prescriptions</field>
            <field name="code">hospital.prescription.order</field>
            <field name="prefix">PRES%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="hospital_lab_test_sequence" model="ir.sequence">
            <field name="name">hospital Lab Test</field>
            <field name="code">ltest_seq</field>
            <field name="prefix">TEST</field>
            <field name="padding">3</field>
        </record>

        <record id="lab_test_sequence" model="ir.sequence">
            <field name="name">Lab Test</field>
            <field name="code">test_seq</field>
            <field name="prefix">LABT</field>
            <field name="padding">3</field>
        </record>

        <record id="new_born_sequence" model="ir.sequence">
            <field name="name">New Born</field>
            <field name="code">new_born_seq</field>
            <field name="prefix">NEW</field>
            <field name="padding">3</field>
        </record>

        <record id="hospital_health_service_sequence" model="ir.sequence">
            <field name="name">hospital Health Service</field>
            <field name="code">hospital_health_ser</field>
            <field name="prefix">MEDSR</field>
            <field name="padding">3</field>
        </record>

        <record id="prescription_invoice_sequence" model="ir.sequence">
            <field name="name">Prescription Invoice</field>
            <field name="code">pres_inv_seq</field>
            <field name="prefix">PRES/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="lab_invoice_sequence" model="ir.sequence">
            <field name="name">Lab Invoice</field>
            <field name="code">lab_inv_seq</field>
            <field name="prefix">LAB/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="hospital_appointments_invoice_sequence" model="ir.sequence">
            <field name="name">hospital Appointments Invoice</field>
            <field name="code">hospital_app_inv_seq</field>
            <field name="prefix">MED/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="hospital_health_service_invoice_sequence" model="ir.sequence">
            <field name="name">Hospital Health Service Invoice</field>
            <field name="code">hospital_health_service_inv_seq</field>
            <field name="prefix">MED/HS/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="hospital_lab_test_invoice_sequence" model="ir.sequence">
            <field name="name">Hospital Lab Test Invoice</field>
            <field name="code">hospital_lab_test_inv_seq</field>
            <field name="prefix">MED/LT/INV/</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
