<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="hospital_patient_perinatal_ovulation_tree_view"
            model="ir.ui.view">
            <field name="name">hospital.patient.perinatal.ovulation.tree.view</field>
            <field name="model">hospital.patient.perinatal.ovulation</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="ovulation_date" />
                    <field name="gestational_weeks" />
                    <field name="hypertansion" />
                    <field name="preclampsia" />
                    <field name="overweight" />
                    <field name="diabetes" />
                    <field name="placenta_previa" />
                    <field name="invasive_placentation" />
                    <field name="vasa_previa" />
                </tree>
            </field>
        </record>

</odoo>
